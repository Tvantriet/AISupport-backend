{
  "name": "app",
  "version": "1.0.0",
  "description": "lwg project",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob dist/*",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc admin:bundle copy-assets",
    "dev:start": "npm-run-all build start",
    "dev": "tsx watch src/index.ts",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-assets": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" node tools/copyAssets.ts",
    "admin:bundle": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" node tools/bundler.ts",
    "knex:migrate:make": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex --knexfile src/database/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex --knexfile src/database/knexfile.ts migrate:latest",
    "knex:migrate:rollback": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex --knexfile src/database/knexfile.ts migrate:rollback",
    "knex:seed:make": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex --knexfile src/database/knexfile.ts seed:make -x ts",
    "knex:seed:run": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" knex --knexfile src/database/knexfile.ts seed:run"
  },
  "author": "lwg",
  "license": "ISC",
  "dependencies": {
    "@adminjs/bundler": "^3.0.0",
    "@adminjs/design-system": "^4.1.1",
    "@adminjs/express": "^6.1.0",
    "@adminjs/import-export": "^3.0.0",
    "@adminjs/themes": "^1.0.1",
    "@adminjs/typeorm": "^5.0.1",
    "@carbon/icons-react": "^11.34.1",
    "@dnd-kit/core": "^6.0.7",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@google-cloud/error-reporting": "^3.0.5",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^6.9.0",
    "@google-cloud/tasks": "^4.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.17",
    "adminjs": "^7.8.2",
    "axios": "^1.6.0",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "connect-typeorm": "^2.0.0",
    "cors": "^2.8.5",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.4.0",
    "express-mysql-session": "^3.0.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "mysql2": "^3.9.1",
    "nodemailer": "^6.9.0",
